  "version": "1.0.6",
  "description": "Enables touchfriendly, swipeable, carousel-like slides through nolimits4web/Swiper",
  "keywords": [
    "ember-addon",
    "swiper",
    "touch",
    "carousel"
  ],
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
