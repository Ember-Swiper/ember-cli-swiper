    "ember-sinon": "^5.0.0",
    "ember-source": "~3.28.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^4.0.0",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
